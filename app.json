{
	"expo": {
		"name": "AnimeThemesMobile",
		"slug": "AnimeThemesMobile",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "animethemesmobile",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.animethemesmobile",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router", "expo-secure-store", "expo-build-properties", "expo-asset"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
